{
  "name": "bingo",
  "version": "1.0.1",
  "description": "JavaScript based Bingo game developed using TDD approach unit testing with Mocha and Chai. Backend with Express/NodeJS and MongoDB for database.",
  "main": "src/app.js",
  "scripts": {
    "test": "mocha --compilers js:@babel/register --require @babel/polyfill --require test/unit/setup.js"
  },
  "jest": {
    "testPathDirs": [
      "back-office-tests"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "automock": true,
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/react-dom",
      "node_modules/react-addons-test-utils",
      "node_modules/fbjs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mihailgaberov/bingo.git"
  },
  "keywords": [
    "bingo",
    "tdd",
    "es6",
    "js",
    "game"
  ],
  "author": "Mihail Gaberov",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "dotenv": "^2.0.0",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.2.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babelify": "^10.0.0",
    "bootstrap-sass": "^3.3.7",
    "browserify": "^16.2.3",
    "canvas": "^2.1.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "del": "^2.2.2",
    "event-emitter-es6": "^1.1.5",
    "express": "^4.16.3",
    "fbemitter": "^2.1.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^3.0.1",
    "gulp-istanbul": "^1.1.3",
    "gulp-mocha": "^3.0.1",
    "gulp-postcss": "^6.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^4.0.1",
    "gulp-server-livereload": "^1.8.4",
    "gulp-sourcemaps": "^1.12.1",
    "immutable": "^3.8.2",
    "invariant": "^2.2.4",
    "jest-cli": "^17.0.3",
    "jsdom": "^11.10.0",
    "lodash": "^4.17.13",
    "mocha": "^3.5.3",
    "mongoose": "^5.0.17",
    "nightwatch": "^0.9.21",
    "node-fetch": "^2.3.0",
    "paper": "^0.10.3",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.6.2",
    "sinon": "^1.17.7",
    "vanilla-modal": "^1.6.5",
